{
  "name": "@ibrokethat/super-iter",
  "description": "super iteration",
  "version": "1.0.0",
  "author": "si@ibrokethat.com",
  "contributors": [
    {
      "name": "Simon Jefford",
      "email": "si@ibrokethat.com"
    },
    {
      "name": "Christos Constandinou",
      "email": "christos@muigui.com"
    }
  ],
  "devDependencies": {
    "babel": "^4.5.5",
    "chai": "latest",
    "mocha": "latest",
    "sinon": "latest"
  },
  "keywords": [
    "ibrokethat",
    "iteration",
    "es6",
    "generators"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ibrokethat/iter"
  },
  "main": "lib/iter.js",
  "scripts": {
    "prepublish": "npm prune && npm run build",
    "build":"./node_modules/.bin/babel --blacklist regenerator,es6.forOf src --out-dir lib" ,
    "test": "./node_modules/.bin/mocha --compilers js:./test/babel --blacklist regenerator,es6.forOf --harmony --colors --recursive -R spec --ui bdd ./test/**/*.test.js --NODE_ENV=test"
  },
  "engines": {
    "node": "0.12.x"
  }
}
